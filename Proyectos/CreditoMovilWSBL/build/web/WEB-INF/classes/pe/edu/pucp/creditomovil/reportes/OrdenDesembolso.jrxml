	<query language="sql"><![CDATA[    SELECT 
        u.nombre,
        u.ap_paterno,
        u.ap_materno,
        u.contrasena,
        c.direccion,
        
        cr.monto,
        cr.num_credito,
        cr.tasa_interes

    FROM creditomovil.usuario u,  creditomovil.credito cr,  creditomovil.cliente_credito crcl, cliente c
    WHERE u.usuario_id = c.usuario_usuario_id and c.codigo_cliente = $P{codCli} and cr.num_credito = $P{codCred} 
    and crcl.credito_num_credito = cr.num_credito 
    and u.documento = crcl.num_documento and u.tipo_doc = crcl.tipo_documento]]></query>
