	<query language="SQL"><![CDATA[    SELECT 
        u.usuario_id,
        u.fecha,
        u.nombre,
        u.ap_paterno,
        u.ap_materno,
        u.contrasena,
        u.fecha_venc,
        u.tipo_doc,
        u.documento,
        u.activo,
        u.ultimo_logeo,
        c.codigo_cliente,
        c.direccion,
        c.telefono,
        c.email,
        c.tipo_cliente,
        c.ranking
    FROM usuario u
    INNER JOIN cliente c ON u.usuario_id = c.usuario_usuario_id
    WHERE c.codigo_cliente = $P{codCli}
    ]]></query>
