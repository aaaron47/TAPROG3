	<property name="com.jaspersoft.studio.data.defaultdataadapter" value="AdapterPUCP"/>
	<parameter name="codCli" class="java.lang.Integer"/>
	<query language="SQL"><![CDATA[    SELECT 
        c.num_credito,
        c.monto,
        c.tasa_interes,
        c.fecha_otorgamiento,
        c.estado,
        c.num_cuotas
    FROM credito AS c
    INNER JOIN cliente_credito AS cc ON c.num_credito = cc.credito_num_credito
    INNER JOIN cliente AS cli ON cli.codigo_cliente = $P{codCli}
    INNER JOIN usuario AS u ON u.usuario_id = cli.usuario_usuario_id
    WHERE cc.num_documento = u.documento 
      AND cc.tipo_documento = u.tipo_doc]]></query>
